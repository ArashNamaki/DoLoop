    <Style x:Key="AppTitle" TargetType="Label">
        <Setter Property="Background" Value="Transparent"/>
        <Setter Property="VerticalAlignment" Value="Center"/>
        <Setter Property="HorizontalAlignment" Value="Center"/>
        <Setter Property="Margin" Value="2,0"/>
        <Setter Property="FontFamily" Value="{StaticResource PrimaryFont}"/>
        <Setter Property="FontStyle" Value="{StaticResource Italic}"/>
        <Setter Property="FontWeight" Value="{StaticResource Thin}"/>
        <Setter Property="FontSize" Value="{StaticResource Tasks}"/>
        <Setter Property="Background" Value="{StaticResource Transparent}"/>
        <Setter Property="Foreground" Value="{StaticResource UIElements}"/>
    </Style>
    
    

    <!--Appbar Buttons-->
    <Style x:Key="AppbarButtonStyles" TargetType="Border">
        <Setter Property="Background" Value="Transparent"/>
        <Setter Property="BorderBrush" Value="Transparent"/>
        <Setter Property="BorderThickness" Value="0"/>
        <Setter Property="VerticalAlignment" Value="Center"/>
        <Setter Property="HorizontalAlignment" Value="Center"/>
        <Setter Property="Padding" Value="0,14"/>
        <Setter Property="Width" Value="auto"/>
        <Setter Property="Height" Value="auto"/>
        <Style.Triggers>
            <Trigger Property="IsMouseOver" Value="True">
                <Setter Property="Background" Value="{StaticResource UIOverlay}"/>
            </Trigger>
        </Style.Triggers>
    </Style>
    
    <Style x:Key="AppbarButtonFontStyles" TargetType="TextBlock">
        <Setter Property="Background" Value="Transparent"/>
        <Setter Property="VerticalAlignment" Value="Center"/>
        <Setter Property="HorizontalAlignment" Value="Center"/>
        <Setter Property="Padding" Value="30,0"/>
        <Setter Property="FontFamily" Value="{StaticResource PrimaryIcon}"/>
        <Setter Property="Background" Value="{StaticResource Transparent}"/>
        <Setter Property="Foreground" Value="{StaticResource UIElements}"/>
    </Style>
    
    <ControlTemplate x:Key="AppbarCloseButton" x:Name="AppClose" TargetType="Button">
        <Border Style="{StaticResource AppbarButtonStyles}">
             <TextBlock Style="{StaticResource AppbarButtonFontStyles}" Text="{StaticResource CloseIcon}"/>
        </Border>
    </ControlTemplate>

    <ControlTemplate x:Key="AppbarMinimizeButton" x:Name="AppMinimize" TargetType="Button">
        <Border Style="{StaticResource AppbarButtonStyles}">
            <TextBlock Style="{StaticResource AppbarButtonFontStyles}" Text="{StaticResource Minus}"/>
        </Border>
    </ControlTemplate>

    

    <!--Window Styles-->
    <Style x:Key="WindowStyles" TargetType="Border">
        <Setter Property="Background" Value="{StaticResource UIBackground}"/>
        <Setter Property="CornerRadius" Value="0"/>
    </Style>
    
    <!--ActiveTab Styles-->
    <Style x:Key="ActiveTabStyles" TargetType="Grid">
        <Setter Property="Margin" Value="24"/>
        <Setter Property="Background" Value="{StaticResource UIElements}"/>
    </Style>

    <!--ActiveBar Styles-->
    <Style x:Key="ActiveBarStyles" TargetType="Grid">
        <Setter Property="Margin" Value="0,0,0,6"/>
        <Setter Property="Background" Value="{StaticResource ImportantTask}"/>
    </Style>

    <!--ActiveListTitle Styles-->
    <Style x:Key="ActiveListTitleStyles" TargetType="Grid">
        <Setter Property="Margin" Value="0"/>
        <Setter Property="Background" Value="{StaticResource ImportantTask}"/>
    </Style>

    <!--BoardTitle Styles-->
    <Style x:Key="BoardTitleStyles" TargetType="Grid">
        <Setter Property="Margin" Value="0"/>
        <Setter Property="Background" Value="{StaticResource ImportantTask}"/>
    </Style>

    <!--Date Styles-->
    <Style x:Key="DateStyles" TargetType="Grid">
        <Setter Property="Margin" Value="0"/>
        <Setter Property="Background" Value="{StaticResource ImportantTask}"/>
    </Style>
    
    <!--Main Styles-->
    <Style x:Key="MainStyles" TargetType="Grid">
        <Setter Property="Margin" Value="0,6,0,0"/>
        <Setter Property="Background" Value="{StaticResource QuickTask}"/>
    </Style>

    <!--SideBar Styles-->
    <Style x:Key="SideBarStyles" TargetType="Grid">
        <Setter Property="Margin" Value="0,0,6,0"/>
        <Setter Property="Background" Value="{StaticResource ImportantTask}"/>
    </Style>

    <!--MainPanel Styles-->
    <Style x:Key="MainPanelStyles" TargetType="Grid">
        <Setter Property="Margin" Value="6,0,0,0"/>
        <Setter Property="Background" Value="{StaticResource ImportantTask}"/>
    </Style>

    <Style x:Key="NormalTask_NoStyleStyles" TargetType="Label">
        <Setter Property="Foreground" Value="{StaticResource NormalTask}"/>
        <Setter Property="FontFamily" Value="{StaticResource PrimaryFont}"/>
        <Setter Property="FontSize" Value="{StaticResource Body}"/>
        <Setter Property="FontStyle" Value="{StaticResource Italic}"/>
        <Setter Property="FontWeight" Value="{StaticResource Regular}"/>
    </Style>
    <ControlTemplate x:Key="NormalTask_NoStyle" TargetType="Label">
        <Border Padding="2,2">
            <Grid VerticalAlignment="Top" HorizontalAlignment="Stretch">
                <Grid.ColumnDefinitions>
                    <ColumnDefinition Width="20"/>
                    <ColumnDefinition Width="*"/>
                    <ColumnDefinition Width="20"/>
                </Grid.ColumnDefinitions>
                <Image Grid.Column="0" Source="{StaticResource CoffeeDrawingImage}"/>
                <Label Grid.Column="1" Style="{StaticResource NormalTask_NoStyleStyles}" Padding="8,0" Content="heeeeelp"/>
                <Image Grid.Column="2" Source="{StaticResource XDrawingImage}" Stretch="UniformToFill" Width="8" Height="8"/>
            </Grid>
        </Border>
    </ControlTemplate> 

</ResourceDictionary>