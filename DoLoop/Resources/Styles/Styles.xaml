    <Style x:Key="DateStyles" TargetType="Grid">
        <Setter Property="Focusable" Value="False"/>
        <Setter Property="Margin" Value="0"/>
        <Setter Property="HorizontalAlignment" Value="Right"/>
        <Setter Property="VerticalAlignment" Value="Center"/>
    </Style>

    <!--Main Styles-->
    <Style x:Key="MainStyles" TargetType="Grid">
        <Setter Property="Focusable" Value="False"/>
        <Setter Property="Margin" Value="0,12,0,0"/>
    </Style>
 
    <!--SideBar Styles-->
    <Style x:Key="SidebarStyles" TargetType="Grid">
        <Setter Property="Focusable" Value="False"/>
        <Setter Property="Margin" Value="0,0,16,0"/>
    </Style>

    <!--MainPanel Styles-->
    <Style x:Key="MainPanelStyles" TargetType="Border">
        <Setter Property="BorderThickness" Value="0,1,0,0"/>
        <Setter Property="BorderBrush" Value="{DynamicResource UIElements}"/>
    </Style>

    <!--TaskType Styles-->
    <Style x:Key="TaskTypeStyles" TargetType="WrapPanel">
        <Setter Property="Focusable" Value="False"/>
        <Setter Property="Margin" Value="0,5"/>
        <Setter Property="VerticalAlignment" Value="Center"/>
        <Style.Resources>
            <Style TargetType="WrapPanel">
                <Setter Property="Margin" Value="0,0,12,0"/>
                <Setter Property="VerticalAlignment" Value="Center"/>
            </Style>
        </Style.Resources>
    </Style>

    <!--Sidebar Stackpanel-->
    <Style x:Key="SidebarStackpanel" TargetType="StackPanel">
        <Setter Property="Focusable" Value="False"/>
        <Setter Property="Margin" Value="0,0,0,0"/>
        <Setter Property="Orientation" Value="Vertical"/>
        <Style.Resources>
            <Style TargetType="Button">
                <Setter Property="Margin" Value="0,12,0,0"/>
            </Style>
            <Style TargetType="Line">
                <Setter Property="X1" Value="0"/>
                <Setter Property="X2" Value="14"/>
                <Setter Property="Margin" Value="0,0,0,6"/>
                <Setter Property="StrokeThickness" Value="1"/>
                <Setter Property="StrokeStartLineCap" Value="Round"/>
                <Setter Property="StrokeEndLineCap" Value="Round"/>
                <Setter Property="HorizontalAlignment" Value="Center"/>
                <Setter Property="Stroke" Value="{DynamicResource UIElements}"/>
            </Style>
        </Style.Resources>
    </Style>


    <!--Components-->
    <Style x:Key="UpeerListLine" TargetType="Border">
        <Setter Property="BorderBrush" Value="{DynamicResource UIElements}"/>
        <Setter Property="BorderThickness" Value="0,1.2,0,0"/>
        <Setter Property="Margin" Value="0,5,0,0"/>
    </Style>
    
    <!--Buttons-->
    <Style x:Key="MiniButtonStyles" TargetType="Border">
        <Setter Property="Focusable" Value="False"/>
        <Setter Property="Background" Value="Transparent"/>
        <Setter Property="BorderBrush" Value="Transparent"/>
        <Setter Property="BorderThickness" Value="0"/>
        <Setter Property="CornerRadius" Value="12"/>
        <Setter Property="VerticalAlignment" Value="Center"/>
        <Setter Property="HorizontalAlignment" Value="Center"/>
        <Setter Property="Width" Value="32"/>
        <Setter Property="Height" Value="32"/>
        <Style.Resources>
            <Style TargetType="TextBlock">
                <Setter Property="Background" Value="Transparent"/>
                <Setter Property="VerticalAlignment" Value="Center"/>
                <Setter Property="HorizontalAlignment" Value="Center"/>
                <Setter Property="Padding" Value="30,0"/>
                <Setter Property="FontFamily" Value="{StaticResource PrimaryIcon}"/>
                <Setter Property="Background" Value="{StaticResource Transparent}"/>
                <Setter Property="Foreground" Value="{DynamicResource UIElements}"/>
            </Style>
        </Style.Resources>
    </Style>
    <ControlTemplate x:Key="MiniButton" TargetType="Button">
        <Border x:Name="MiniButton" Style="{DynamicResource MiniButtonStyles}">
            <TextBlock x:Name="MiniButton_Icon" Text="{TemplateBinding Property=ContentStringFormat}"/>
        </Border>
        <ControlTemplate.Triggers>
            <Trigger Property="IsMouseOver" Value="True">
                <Setter TargetName="MiniButton" Property="Background" Value="{DynamicResource UIElements}"/>
                <Setter TargetName="MiniButton_Icon" Property="Foreground" Value="{DynamicResource UIBackground}"/>
            </Trigger>
        </ControlTemplate.Triggers>
    </ControlTemplate>

    <!--Main Buttons-->
    <Style x:Key="ButtonStyle" TargetType="Border">
        <Setter Property="Focusable" Value="False"/>
        <Setter Property="Width" Value="40"/>
        <Setter Property="Height" Value="40"/>
        <Setter Property="CornerRadius" Value="16"/>
        <Setter Property="HorizontalAlignment" Value="Center"/>
        <Setter Property="VerticalAlignment" Value="Center"/>
        <Setter Property="Background" Value="{DynamicResource Transparent}"/>
        <Setter Property="BorderBrush" Value="{DynamicResource UIElements}"/>
        <Setter Property="BorderThickness" Value="1.2"/>
        <Style.Resources>
            <Style TargetType="TextBlock">
                <Setter Property="Focusable" Value="False"/>
                <Setter Property="Foreground" Value="{DynamicResource UIElements}"/>
                <Setter Property="FontFamily" Value="{StaticResource PrimaryIcon}"/>
                <Setter Property="FontSize" Value="{StaticResource Body}"/>
                <Setter Property="FontStyle" Value="{StaticResource Normal}"/>
                <Setter Property="HorizontalAlignment" Value="Center"/>
                <Setter Property="VerticalAlignment" Value="Center"/>
            </Style>
        </Style.Resources>
    </Style>
    <ControlTemplate x:Key="BoardButton" TargetType="Button">
        <Border x:Name="ButtonBorder_Board" Style="{DynamicResource ButtonStyle}">
            <TextBlock x:Name="BoardButton_Icon" Text="{TemplateBinding Property=ContentStringFormat}"/>
        </Border>
        <ControlTemplate.Triggers>
            <Trigger Property="IsMouseOver" Value="True">
                <Setter TargetName="ButtonBorder_Board" Property="Background" Value="{DynamicResource UIElements}"/>
                <Setter TargetName="BoardButton_Icon" Property="Foreground" Value="{DynamicResource UIBackground}"/>
            </Trigger>
        </ControlTemplate.Triggers>
    </ControlTemplate>

    <Style x:Key="BoardIconStyle" TargetType="Border">
        <Setter Property="Focusable" Value="False"/>
        <Setter Property="Width" Value="40"/>
        <Setter Property="Height" Value="40"/>
        <Setter Property="CornerRadius" Value="16"/>
        <Setter Property="HorizontalAlignment" Value="Center"/>
        <Setter Property="VerticalAlignment" Value="Center"/>
        <Setter Property="Background" Value="{DynamicResource UIElements}"/>
        <Setter Property="BorderBrush" Value="{DynamicResource UIElements}"/>
        <Setter Property="BorderThickness" Value="1.2"/>
        <Style.Resources>
            <Style TargetType="TextBlock">
                <Setter Property="Foreground" Value="{DynamicResource UIForeground}"/>
                <Setter Property="FontFamily" Value="{StaticResource PrimaryIcon}"/>
                <Setter Property="FontSize" Value="{StaticResource Body}"/>
                <Setter Property="FontStyle" Value="{StaticResource Normal}"/>
                <Setter Property="HorizontalAlignment" Value="Center"/>
                <Setter Property="VerticalAlignment" Value="Center"/>
            </Style>
        </Style.Resources>
    </Style>
    <ControlTemplate x:Key="BoardIconButton" TargetType="Button">
        <Grid>
            <Border Name="ShadowEffect" Background="{DynamicResource UIElements}" CornerRadius="16" Opacity="0.3">
                <Border.Effect>
                    <BlurEffect Radius="16" KernelType="Box"/>
                </Border.Effect>
            </Border>
            <Border x:Name="ButtonBorder_Board" Style="{DynamicResource BoardIconStyle}">
                <TextBlock x:Name="BoardButton_Icon" Text="{TemplateBinding Property=ContentStringFormat}"/>
            </Border>
        </Grid>
        <ControlTemplate.Triggers>
            <Trigger Property="IsMouseOver" Value="True">
                    <Setter TargetName="ShadowEffect" Property="Opacity" Value="0.6"/>
            </Trigger>
        </ControlTemplate.Triggers>
    </ControlTemplate>

    <!--Sidebar Small Buttons-->
    <Style x:Key="SmallButtonStyle" TargetType="Border">
        <Setter Property="Focusable" Value="False"/>
        <Setter Property="Width" Value="32"/>
        <Setter Property="Height" Value="32"/>
        <Setter Property="CornerRadius" Value="12"/>
        <Setter Property="HorizontalAlignment" Value="Center"/>
        <Setter Property="VerticalAlignment" Value="Center"/>
        <Setter Property="Background" Value="{DynamicResource Transparent}"/>
        <Setter Property="BorderBrush" Value="{DynamicResource UIElements}"/>
        <Setter Property="BorderThickness" Value="1.2"/>
        <Style.Resources>
            <Style TargetType="TextBlock">
                <Setter Property="Foreground" Value="{DynamicResource UIElements}"/>
                <Setter Property="FontFamily" Value="{StaticResource PrimaryIcon}"/>
                <Setter Property="FontSize" Value="{StaticResource Small}"/>
                <Setter Property="FontStyle" Value="{StaticResource Normal}"/>
                <Setter Property="HorizontalAlignment" Value="Center"/>
                <Setter Property="VerticalAlignment" Value="Center"/>
            </Style>
        </Style.Resources>
    </Style>
    <ControlTemplate x:Key="SmallBoardButton" TargetType="Button">
        <Border x:Name="SmallButtonBorder_Board" Style="{DynamicResource SmallButtonStyle}">
            <TextBlock x:Name="SmallBoardButton_Icon" Text="{TemplateBinding Property=ContentStringFormat}"/>
        </Border>
        <ControlTemplate.Triggers>
            <Trigger Property="IsMouseOver" Value="True">
                <Setter TargetName="SmallButtonBorder_Board" Property="Background" Value="{DynamicResource UIElements}"/>
                <Setter TargetName="SmallBoardButton_Icon" Property="Foreground" Value="{DynamicResource UIBackground}"/>
            </Trigger>
        </ControlTemplate.Triggers>
    </ControlTemplate>


    <!--Input-->
    <Style x:Key="TaskInputStyles" TargetType="Border">
        <Setter Property="CornerRadius" Value="16"/>
        <Setter Property="Padding" Value="10"/>
        <Setter Property="VerticalAlignment" Value="Center"/>
        <Setter Property="Background" Value="{DynamicResource Transparent}"/>
        <Setter Property="BorderBrush" Value="{DynamicResource UIElements}"/>
        <Setter Property="BorderThickness" Value="1.2"/>
        <Style.Resources>
            <Style TargetType="Grid">
                <Setter Property="VerticalAlignment" Value="Center"/>
                <Setter Property="ClipToBounds" Value="True"/>
            </Style>
        </Style.Resources>
    </Style>
    <Style x:Key="TaskInputTextStyles" TargetType="TextBox">
        <Setter Property="FontFamily" Value="{StaticResource PrimaryFont}"/>
        <Setter Property="FontSize" Value="{StaticResource Body}"/>
        <Setter Property="FontStyle" Value="{StaticResource Italic}"/>
        <Setter Property="Foreground" Value="{DynamicResource UIElements}"/>
        <Setter Property="Background" Value="{DynamicResource Transparent}"/>
        <Setter Property="BorderBrush" Value="{DynamicResource UIElements}"/>
        <Setter Property="BorderThickness" Value="0"/>
        <Setter Property="TextWrapping" Value="Wrap"/>
        <Setter Property="ClipToBounds" Value="True"/>
        <Setter Property="MaxHeight" Value="40"/>
        <Setter Property="VerticalAlignment" Value="Center"/>
        <Setter Property="Margin" Value="0,0,8,0"/>
    </Style>
    
    <!--RadioButton-->
    <Style x:Key="TaskRadioStyles" TargetType="Border">
        <Setter Property="Focusable" Value="False"/>
        <Setter Property="Width" Value="14"/>
        <Setter Property="Height" Value="14"/>
        <Setter Property="CornerRadius" Value="5"/>
        <Setter Property="BorderThickness" Value="1"/>
        <Setter Property="HorizontalAlignment" Value="Center"/>
        <Setter Property="VerticalAlignment" Value="Center"/>
    </Style>
    <ControlTemplate x:Key="TaskRadio" TargetType="RadioButton">
        <Grid>
            <Border x:Name="RadioShadowEffect" Background="{DynamicResource Transparent}" CornerRadius="16" Opacity="0.3">
                <Border.Effect>
                    <BlurEffect Radius="10.0" KernelType="Box"/>
                </Border.Effect>
            </Border>
            <Border x:Name="Border" Style="{DynamicResource TaskRadioStyles}" 
            BorderBrush="{TemplateBinding BorderBrush}" Background="{DynamicResource Transparent}"/>
        </Grid>
        <ControlTemplate.Triggers>
            <Trigger Property="IsChecked" Value="True">
                <Setter TargetName="Border" Property="Background" Value="{Binding RelativeSource={RelativeSource TemplatedParent}, Path=Background}"/>
                <Setter TargetName="RadioShadowEffect" Property="Background" Value="{Binding RelativeSource={RelativeSource TemplatedParent}, Path=Background}"/>
            </Trigger>
            <Trigger Property="IsEnabled" Value="False">
                <Setter TargetName="Border" Property="Opacity" Value="0.3"/>
                <Setter TargetName="Border" Property="Background" Value="{DynamicResource Transparent}"/>
            </Trigger>
        </ControlTemplate.Triggers>
    </ControlTemplate>

    <Style x:Key="TaskRadioTextStyles" TargetType="Label">
        <Setter Property="Visibility" Value="Visible"/>
        <Setter Property="Background" Value="{DynamicResource Transparent}"/>
        <Setter Property="FontFamily" Value="{DynamicResource PrimaryFont}"/>
        <Setter Property="FontSize" Value="{StaticResource Small}"/>
        <Setter Property="FontStyle" Value="{DynamicResource Italic}"/>
        <Setter Property="Margin" Value="2,0,0,0"/>
        <Setter Property="Padding" Value="0"/>
        <Setter Property="HorizontalAlignment" Value="Center"/>
        <Setter Property="VerticalAlignment" Value="Center"/>
        <Setter Property="VerticalContentAlignment" Value="Center"/>
        <Style.Triggers>
            <DataTrigger Binding="{Binding RelativeSource={RelativeSource AncestorType=Grid}, Path=ActualWidth}" Value="376">
                <Setter Property="Visibility" Value="Collapsed"/>
            </DataTrigger>
        </Style.Triggers>
    </Style>
    <ControlTemplate x:Key="TaskRadioText" TargetType="Label">
        <WrapPanel>
            <Label Style="{DynamicResource TaskRadioTextStyles}" Foreground="{TemplateBinding Foreground}"
               Content="{TemplateBinding Content}"/>
        </WrapPanel>
    </ControlTemplate>

    <Style x:Key="TaskRadioIconStyles" TargetType="Label">
        <Setter Property="Focusable" Value="False"/>
        <Setter Property="Background" Value="{DynamicResource Transparent}"/>
        <Setter Property="FontFamily" Value="{DynamicResource PrimaryIcon}"/>
        <Setter Property="FontSize" Value="{StaticResource Small}"/>
        <Setter Property="FontStyle" Value="{DynamicResource Normal}"/>
        <Setter Property="Margin" Value="2,0,0,0"/>
        <Setter Property="Padding" Value="0"/>
        <Setter Property="Width" Value="20"/>
        <Setter Property="Height" Value="20"/>
        <Setter Property="HorizontalContentAlignment" Value="Center"/>
        <Setter Property="VerticalContentAlignment" Value="Center"/>
        <Setter Property="HorizontalAlignment" Value="Center"/>
        <Setter Property="VerticalAlignment" Value="Center"/>
    </Style>
    <ControlTemplate x:Key="TaskRadioIcon" TargetType="Label">
        <WrapPanel>
            <Label Style="{DynamicResource TaskRadioIconStyles}" Foreground="{TemplateBinding Foreground}"
           Content="{TemplateBinding Content}"/>
        </WrapPanel>
    </ControlTemplate>

    <!--Titles-->
    <Style x:Key="BoardTitleTextStyles" TargetType="TextBlock">
        <Setter Property="Focusable" Value="False"/>
        <Setter Property="Foreground" Value="{DynamicResource UIElements}"/>
        <Setter Property="FontFamily" Value="{StaticResource PrimaryFont}"/>
        <Setter Property="FontSize" Value="{StaticResource Titles}"/>
        <Setter Property="FontStyle" Value="{StaticResource Normal}"/>
        <Setter Property="FontWeight" Value="{StaticResource Regular}"/>
        <Setter Property="Margin" Value="4,0"/>
        <Setter Property="VerticalAlignment" Value="Center"/>
    </Style>

    <Style x:Key="DateTextStyles" TargetType="TextBlock">
        <Setter Property="Focusable" Value="False"/>
        <Setter Property="Foreground" Value="{DynamicResource UIElements}"/>
        <Setter Property="FontFamily" Value="{StaticResource PrimaryFont}"/>
        <Setter Property="FontSize" Value="{StaticResource Body}"/>
        <Setter Property="FontStyle" Value="{StaticResource Italic}"/>
        <Setter Property="FontWeight" Value="{StaticResource Thin}"/>
        <Setter Property="Margin" Value="4,0"/>
        <Setter Property="VerticalAlignment" Value="Center"/>
    </Style>
    
    <!--Tasks-->
    <Style x:Key="TaskItemSimpleStyles" TargetType="Border">
        <Setter Property="Focusable" Value="False"/>
        <Setter Property="Background" Value="{DynamicResource Transparent}"/>
        <Setter Property="Padding" Value="0,4"/>
        <Setter Property="Margin" Value="0,2"/>
        <Setter Property="Cursor" Value="Hand"/>
        <Setter Property="VerticalAlignment" Value="Center"/>
        <Setter Property="HorizontalAlignment" Value="Stretch"/>
        <Style.Resources>
            <Style TargetType="Label">
                <Setter Property="Focusable" Value="False"/>
                <Setter Property="Foreground" Value="{DynamicResource UIElements}"/>
                <Setter Property="FontFamily" Value="{StaticResource PrimaryIcon}"/>
                <Setter Property="FontSize" Value="{StaticResource Body}"/>
                <Setter Property="FontStyle" Value="{StaticResource Normal}"/>
                <Setter Property="FontWeight" Value="{StaticResource Regular}"/>
                <Setter Property="VerticalAlignment" Value="Top"/>
                <Setter Property="HorizontalAlignment" Value="Center"/>
            </Style>
            <Style TargetType="TextBlock">
                <Setter Property="Focusable" Value="False"/>
                <Setter Property="Foreground" Value="{DynamicResource UIElements}"/>
                <Setter Property="FontFamily" Value="{StaticResource PrimaryFont}"/>
                <Setter Property="FontSize" Value="{StaticResource Tasks}"/>
                <Setter Property="FontStyle" Value="{StaticResource Italic}"/>
                <Setter Property="FontWeight" Value="{StaticResource Regular}"/>
                <Setter Property="TextWrapping" Value="Wrap"/>
                <Setter Property="VerticalAlignment" Value="Center"/>
                <Setter Property="HorizontalAlignment" Value="Stretch"/>
            </Style>
            <Style TargetType="Button">
                <Setter Property="Focusable" Value="False"/>
                <Setter Property="BorderThickness" Value="0"/>
                <Setter Property="VerticalAlignment" Value="Top"/>
                <Setter Property="HorizontalAlignment" Value="Center"/>
                <Setter Property="FontFamily" Value="{StaticResource PrimaryIcon}"/>
                <Setter Property="FontSize" Value="{StaticResource TaskItemDelete}"/>
                <Setter Property="Background" Value="{StaticResource Transparent}"/>
                <Setter Property="Foreground" Value="{DynamicResource UIElements}"/>
            </Style>
        </Style.Resources>
    </Style>
    
</ResourceDictionary>